        "# Load model directly\n",
        "from transformers import AutoTokenizer, AutoModelForCausalLM\n",
        "\n",
        "tokenizer = AutoTokenizer.from_pretrained(\"JacopoCirica/Llama_3.1_reasoning_italian_16bit\")\n",
        "model = AutoModelForCausalLM.from_pretrained(\"JacopoCirica/Llama_3.1_reasoning_italian_16bit\")\n",
        "messages = [\n",
        "    {\"role\" : \"system\", \"content\" : SYSTEM_PROMPT},\n",
        "    {\"role\" : \"user\", \"content\" : \"Le anatre di Janet depongono 16 uova al giorno. Ne mangia tre per colazione ogni mattina e cuoce muffin per i suoi amici ogni giorno con quattro. Vende il resto al mercato contadino ogni giorno per 2 dollari per ogni uovo d'anatra fresco. Quanto guadagna in dollari ogni giorno al mercato contadino?\"},\n",
        "]\n",
        "inputs = tokenizer.apply_chat_template(\n",
        "\tmessages,\n",
        "\tadd_generation_prompt=True,\n",
        "\ttokenize=True,\n",
        "\treturn_dict=True,\n",
        "\treturn_tensors=\"pt\",\n",
        ").to(model.device)\n",
        "\n",
        "outputs = model.generate(**inputs, max_new_tokens=400)\n",
        "print(tokenizer.decode(outputs[0][inputs[\"input_ids\"].shape[-1]:]))"
