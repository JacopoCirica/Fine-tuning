      "source": [
        "inputs = tokenizer(prompt, return_tensors=\"pt\").to(\"cuda\")\n",
        "\n",
        "with torch.no_grad():\n",
        "    output_ids = model.generate(\n",
        "        **inputs,\n",
        "        max_new_tokens = 512,\n",
        "        temperature    = 0.1,\n",
        "        top_p          = 0.95,\n",
        "        do_sample      = True,\n",
        "    )\n",
        "\n",
        "text = tokenizer.decode(output_ids[0], skip_special_tokens=True)\n",
        "print(text)\n"
      ],
